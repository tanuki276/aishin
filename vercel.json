{
  "builds": [
    {
      "src": "public/index.html",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/data/(.*)",
      "dest": "/data/$1"
    },
    {
      "src": "/dict/(.*)",
      "dest": "/dict/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
